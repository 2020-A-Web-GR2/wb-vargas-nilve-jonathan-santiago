<%- include('../compartido/navbar/navbar')%>

<div class="container main-container">

    <div align="center">
        <h1>Mi Cuenta</h1>
        <hr>
        <% const existeUsuario = typeof usuarioCuenta != 'undefined'%>

        <div class="card"  style="width: 40%;">

            <form>
                <fieldset disabled>

                    <div class="row">

                        <div class="col-md-12">
                            <br><br>

                            <div class="form-group">
                                <label for="cedulaID">Cedula</label>
                                <input id="cedulaID" type="number"
                                       class="form-control"
                                       value="<%=usuarioCuenta.cedula%>">
                            </div>

                            <div class="form-group">
                                <label for="nombreID">Nombre</label>
                                <input id="nombreID" type="text"
                                       class="form-control"
                                       value="<%=usuarioCuenta.nombre%>">
                            </div>

                            <div class="form-group">
                                <label for="apellidoID">Apellido</label>
                                <input id="apellidoID" type="text"
                                       class="form-control"
                                       value="<%=usuarioCuenta.apellido%>">
                            </div>

                            <div class="form-group">
                                <label for="tipoID">Tipo</label>
                                <input id="tipoID" type="text"
                                       class="form-control"
                                       value="<%=usuarioCuenta.tipo%>">
                            </div>

                            <div class="form-group">
                                <label for="ciudadID">Ciudad</label>
                                <input id="ciudadID" type="text"
                                       class="form-control"
                                       value="<%=usuarioCuenta.ciudad%>">
                            </div>

                            <div class="form-group">
                                <label for="loginID">Login</label>
                                <input id="loginID" type="text"
                                       class="form-control"
                                       value="<%=usuarioCuenta.login%>">
                            </div>
                        </div>
                    </div>

                </fieldset>

                <div class="col-md-6">
                    <a href="/bioapp/home"
                       class="btn btn-danger btn-block"
                       role="button">Regresar</a>
                </div>
            </form>
        </div>

    </div>




</div>
